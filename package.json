{
  "name": "royal-academy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build-css": "tailwindcss build style.css -o css/style.css",
    "build": "dotenv -e .env.production -- next build",
    "start": "next start",
    "type-check": "tsc --noemit",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "npx prisma": "prisma",
    "start:migrate": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "20.1.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "jose": "^4.14.4",
    "next": "13.4.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss-import": "^15.1.0",
    "prisma": "^4.15.0"
  }
}
